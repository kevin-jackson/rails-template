= simple_form_for [:admin, setup_client(@client)], wrapper: "horizontal", html: { class: "form-horizontal"}, url: url do |f|

  .form-inputs
    = f.input :name

    = render partial: "address/form", locals: {address: f}

  .form-actions
    = f.button :submit
    %span.or or
    = link_to "Cancel", admin_clients_path
